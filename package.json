{
  "name": "observer-backend",
  "version": "0.0.1",
  "description": "",
  "main": "scheduler.js",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "cron": "^1.1.0",
    "fbgraph": "^1.1.0",
    "foreman": "^1.4.1",
    "jsonfile": "^2.2.3",
    "moment": "^2.12.0",
    "prettyjson": "^1.1.3",
    "q": "^1.4.1",
    "underscore": "^1.8.3",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.0.1"
  },
  "devDependencies": {
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  },
  "scripts": {
    "start": "node ./build/scheduler.js",
    "build": "babel ./server -d ./build --source-maps --copy-files",
    "watch": "babel ./server -d ./build --source-maps --copy-files --watch",
    "clean": "rm -rf ./build",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --reporter spec 'test/**/*.test.js'",
    "test-debug": "NODE_ENV=test mocha debug --compilers js:babel-register --reporter spec 'test/**/*.test.js'",
    "coverage": "npm run analyze-coverage && npm run check-coverage --silent",
    "analyze-coverage": "NODE_ENV=test ./node_modules/.bin/babel-node node_modules/.bin/istanbul cover node_modules/.bin/_mocha  'test/**/*.js'",
    "check-coverage": "node node_modules/.bin/istanbul check-coverage --branches 100 --lines 100 --function 100",
    "lint": "eslint ./server ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnjs/observer-backend.git"
  },
  "author": "d.m.michalski@gmail.com",
  "license": "MIT",
